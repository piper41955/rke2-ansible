.:53 {
    errors
    health {
        lameduck 10s
    }
    ready
    kubernetes cluster.local cluster.local in-addr.arpa ip6.arpa {
        pods insecure
        fallthrough in-addr.arpa ip6.arpa
        ttl 30
    }
    prometheus 0.0.0.0:9153
    forward . "10.31.10.10" "10.31.10.11" {
        max_concurrent 1000
        prefer_udp
        policy sequential
    }
    cache 30
    loop
    reload
    loadbalance
}

